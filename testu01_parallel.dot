      Gen2 [label = "Gen2"]
      Gen2 -> "Test2" [label = "S0"]
      Slave2 -> "Test2" -> Output2
      "Test2" -> Report2
    }
    subgraph cluster_test_process3{
      fontsize = 24
      label = ""
      {rank = same; Gen3; Test3;}
      Slave3 [label = "slave_process.out"]
      Report3 [label = "Report"]
